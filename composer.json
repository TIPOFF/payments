{
    "name": "tipoff/payments",
    "description": "Laravel Package for implementing Ecommerce payments",
    "keywords": [
        "tipoff",
        "payments"
    ],
    "homepage": "https://github.com/tipoff/payments",
    "license": "MIT",
    "authors": [
        {
            "name": "Tipoff",
            "email": "packages@tipoff.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "tipoff/authorization": "^2.5.0",
        "tipoff/checkout": "2.3.2",
        "tipoff/invoices": "2.0.0",
        "tipoff/locations": "^2.4.0",
        "tipoff/refunds": "^2.1.0",
        "tipoff/support": "^1.5.10"
    },
    "require-dev": {
        "tipoff/test-support": "^1.2.0"
    },
    "autoload": {
        "psr-4": {
            "Tipoff\\Payments\\": "src",
            "Tipoff\\Payments\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tipoff\\Payments\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tipoff\\Payments\\PaymentsServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
